%tr
  %td= link_to(instructable.name, edit_user_instructable_path(instructable.user, instructable))
  %td= instructable.tract
  %td= instructable.repeat_count
  %td= instructable.formatted_culture_and_topic
  %td= instructable.approved? ? "Yes" : "No"
  %td= instructable.instances.pluck(:start_time).join(", ")
